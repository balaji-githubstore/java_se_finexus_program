package com.finexus.inheritance;

class Father{
	
	public void fatherStyle()
	{
		System.out.println("Father Style!!");
	}
	
}

class Son 
{
	
	public void sonStyle()
	{
		System.out.println("Son Style!!");
	}
}





public class InheritanceDemo {
	public static void main(String[] args) {
		
		
	}

}
