package com.finexus.collections;

import java.util.ArrayList;

public class Demo2ArrayListNonGeneric {

	public static void main(String[] args) {
		
		//store all record as an Object type
		ArrayList lists=new ArrayList();
		
		lists.add(45);
		lists.add(45.2);
		lists.add("John");
		
		int x=lists
	}
}
