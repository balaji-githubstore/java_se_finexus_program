package com.finexus.stringbuilder;

public class Demo1 {
	
	public static void main(String[] args) {
		
		//immutable
		String color="red"; 
		
		
		color="yellow";
		color="blue";
		color=color+" color";
		
		System.out.println(color);
		
		StringBuilder strBuilder=new StringBuilder("red");
		strBuilder.append("")
		
	}

}
