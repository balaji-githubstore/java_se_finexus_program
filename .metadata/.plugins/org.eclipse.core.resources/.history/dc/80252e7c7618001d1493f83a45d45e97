package com.finexus.employee_encap;

public class EmployeeDemo {

	public static void main(String[] args) {
		
		Employee.companyName="Finexus";
		
		Employee emp1=new Employee();
		Employee emp2=new Employee();

		
		emp1.empId=101;
		emp1.empName="John";
		emp1.empSalary=8000;

		emp2.empId=102;
		emp2.empName="Peter";
		emp2.empSalary=5000;



		


	}

}
