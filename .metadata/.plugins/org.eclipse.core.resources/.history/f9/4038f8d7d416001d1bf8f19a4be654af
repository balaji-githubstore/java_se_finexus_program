package com.finexus.logics;

public class Demo1ConditionalStatement {
	
	public static void main(String[] args) {
		
		int num=0;
		
		if(num>0)
		{
			System.out.println(num+" is positive");
		}
		else if(num==0)
		{
			System.out.println("It's zero");
		}
		else 
		{
			System.out.println(num+" is negative");
		}
		
	}

}
