public class Demo1 {

	public static void main(String[] args) {

		int radius = 10;

		double result = Demo1.areaOfCircle(9);
		System.out.println(result);

		result = Demo1.areaOfCircle(10);
		System.out.println(result);

		result = Demo1.areaOfRectangle(10, 10.2);
