package com.finexus.datatypes;

public class ArrayDemo2 {

	public static void main(String[] args) {

		// percentage of 5 student - 10,20,30,40,50

		int[] numbers = new int[5];
		
		numbers[0]=10;
		numbers[1]=20;
		numbers[2]=30;
		numbers[3]=40;
		numbers[4]=50;
		
		System.out.println(numbers[0]);

	}

}
